
<div>
  I'm a div
</div>

<%= form_for @user, :action => "create", :method => :post do |f| %>
    <%= f.label :username , "Enter a Username" %>
    <%= f.text_field :username %>
    <%= f.label :password , "Enter a Password" %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation , "Enter Confirmation" %>
    <%= f.password_field :password_confirmation %>
    <%= f.label :email , "Enter Email" %>
    <%= f.text_field :email %>
    <%= f.submit "Create new account" %>
<% end %>
